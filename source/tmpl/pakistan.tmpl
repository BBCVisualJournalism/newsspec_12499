<section class="section section-pakistan">
    <div class="section_cover_container section_cover_container-a">
        <div class="section_cover">
            <img src="{{= pathStatic }}/{{= vocab_dir }}/img/pakistan/pakistan_cover.png" alt="">
        </div>
    </div>

    <div class="section_container">
        <div class="profile_photo profile_photo-pakistan">
            <img src="{{= pathStatic }}/{{= vocab_dir }}/img/pakistan/pakistan_profile_630.jpg" alt="{{= pakistan_profile_img_alt }}">
        </div>
        <h2 class="section_title"><span>{{= pakistan_subtitle }}</span></h2>
        <div class="clear"></div>
        <div class="section_text">
            <p class="section_paragraph">{{= pakistan_para_1 }}</p>
            <p class="section_paragraph">{{= pakistan_para_2 }}</p>
        </div>
    </div>

    <div class="section_container-media">
        <div class="section_video_container" id="section_video_container-pakistan">
            <div class="section_video"></div>
            <div class="section_video_overlay">
                <div class="section_video_overlay_play" aria-label="{{= pakistan_video_play_alt }}"></div>
                <div class="section_video_overlay_label">{{= pakistan_video_title }}</div>
            </div>
        </div>
        <div class="section_photo section_photo-one">
            <img src="{{= pathStatic }}/{{= vocab_dir }}/img/pakistan/text_messages.png" alt="{{= pakistan_img_1_alt }}">
        </div>
        <div class="section_photo section_photo-two">
            <img src="{{= pathStatic }}/{{= vocab_dir }}/img/pakistan/silence.png" alt="{{= pakistan_img_2_alt }}">
        </div>
    </div>
    
    <div class="clear"></div>
</section>