<section class="section section-pakistan">
    <div class="section_cover section_cover_pink section_cover-pakistan"></div>

    <div class="section_container">
        <div class="profile_photo profile_photo-pakistan">
            <img src="{{= pathStatic }}/{{= vocab_dir }}/img/pakistan/pakistan_profile_630.jpg" alt="{{= pakistan_profile_img_alt }}">
        </div>
        <h2 class="section_title">{{= pakistan_subtitle }}</h2>

        <p class="section_paragraph">{{= pakistan_para_1 }}</p>
        <p class="section_paragraph">{{= pakistan_para_2 }}</p>

        <div class="section_video_container section_video_container-pakistan">
            <div class="section_video"></div>
            <div class="section_video_overlay">
                <button class="section_video_overlay_play" aria-label="{{= pakistan_video_play_alt }}"></button>
                <div class="section_video_overlay_label">{{= pakistan_video_title }}</div>
            </div>
        </div>
        <img src="{{= pathStatic }}/{{= vocab_dir }}/img/pakistan/text_messages.svg" data-fallback="{{= pathStatic }}/{{= vocab_dir }}/img/pakistan/text_messages.png" alt="{{= pakistan_img_1_alt }}" class="section_photo section_photo-one">
        <img src="{{= pathStatic }}/{{= vocab_dir }}/img/pakistan/silence.svg" data-fallback="{{= pathStatic }}/{{= vocab_dir }}/img/pakistan/silence.png" alt="{{= pakistan_img_2_alt }}" class="section_photo section_photo-two">
    </div>
</section>